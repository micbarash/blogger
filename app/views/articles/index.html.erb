<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article) %>
    </li>
  <% end %>
</ul>
<% if logged_in? %>
	<%= link_to "Create a New Aticle", new_article_path, class: "new_article" %><br />
<% end %>
<%= link_to "View Tags", tags_path %>